{
  "name": "combojs",
  "version": "0.0.1",
  "description": "A combobox for intelligent, responsive search and filtering.",
  "main": "combojs.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "stylus": "stylus -m -w -o lib/build lib/combo.styl",
    "coffee_test": "coffee -o lib/build -cmw lib/combo.coffee",
    "coffee_lib": "coffee -o tests/build -cmw tests/",
    "clean": "del ./lib/build /Q& del ./tests/results /Q& del ./tests/build /Q",
    "start": "npm run clean& start npm run stylus& start npm run coffee_lib& start npm run coffee_test",
    "nightwatch": "nightwatch --config ./tests/nightwatch.json",
    "_pretest": "npm run start",
    "test": "npm run nightwatch",
    "watch": "watch \"npm test\" ./tests/build/ --wait=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asgerhallas/combojs"
  },
  "keywords": [
    "searchbox",
    "combobox",
    "reponsive",
    "filter"
  ],
  "author": "Asger Hallas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asgerhallas/combojs/issues"
  },
  "homepage": "https://github.com/asgerhallas/combojs",
  "dependencies": {
    "jquery": "^2.1.1",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "should": "^4.3.0",
    "stylus": "^0.49.3"
  }
}
